<Application x:Class="StarHue.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:StarHue"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/CloseButton.xaml"/>
                <ResourceDictionary Source="Resources/CopyButton.xaml"/>
                <ResourceDictionary Source="Resources/ColorPickButton.xaml"/>
                <ResourceDictionary Source="Resources/HyperlinkStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
